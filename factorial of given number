\\write a c programe to calculate factorial of given number
#include <stdio.h>

int main() {
int n,i;
long fact=1;
  printf("enter n :");
  scanf("%d",&n);
  if( n<0){
      printf("factorial number is negative is does not exist\n");}
     else{ 
     for(i=1; i<=n; i++){
         fact *=i;
     }
         printf("factorial of %d=%11d\n",n,fact);
     }   
         
  
    return 0;
}
