//To print arthimetic operators using switch case
#include <stdio.h>

int main() {
int a,b;
char ch;
    printf("enter 2 no for arthimetic operators;");
    scanf("%d %d ",&a,&b);
    printf("enter your operator (+,-,*,/)");
    scanf("%c",&ch);
    switch(ch){
        case'+':printf("addition is %d",a+b);
        break;
        case'-':printf("subraction is %d",a-b);
        break;
        case'*':printf("multiply is %d",a*b);
        break;
        case'/':printf("division is %d",a/b);
        break;
        default:printf("wrong statement");
    }
        

    return 0;
}
